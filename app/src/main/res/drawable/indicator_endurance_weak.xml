<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/background_progress_bar">
        <shape>
            <corners android:radius="2dp"/>
            <solid android:color="@color/black"/>
        </shape>
    </item>
    <item android:id="@+id/value_progress_bar">
        <clip>
            <shape>
                <corners android:radius="2dp"/>
                <solid android:color="@color/weakness"/>
            </shape>
        </clip>
    </item>
</layer-list>